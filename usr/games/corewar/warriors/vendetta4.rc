; vendetta4
; Tuomo 'vendu' Venäläinen
; bidirectional, silly multitasking bomber
dat		#2	; backward copy offset
dat		#-4	; forward copy offset
sub	#7,	-2	; adjust backward copy offset
add	#7,	-2	; adjust forward copy offset
mov	-4	@-4	; backward copy
mov	-4	@-4	; forward copy
spl		-4	; spawn at SUB
jmp		-5	; jump to SUB
